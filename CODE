#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int n,m,k;
		cin>>n>>m>>k;
		int a[n][k];
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<k;j++)
				cin>>a[i][j];
		}
		for(int i=0;i<n;i++)
		{
			int maxN=0,ans=0;
			map<int,int>m;
			map<int,int>::iterator it;
			for(int j=0;j<k;j++)
			{
				m[a[i][j]]++;
			}
			for(it=m.begin();it!=m.end();it++)
			{
				if(maxN<it->second){
					maxN=it->second;
					ans = it->first;
				}
					
			}
			cout<<ans<<" ";
			m.clear();
		}
		cout<<endl;
	}
	return 0;
}
